			MOVS R3,R4
			ADDS R0,R1,R2
			LSRS R0,R0,#1
			MULS R7,R4,R7
			CMPS R7,R0
			BNE ELSEIF
			MOVS R5,#1
	 ELSEIF:
			CMP R7,R0
